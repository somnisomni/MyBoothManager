    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "eslint": "^9.16.0",
    "execa": "^9.5.2",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "=5.6.3",
    "tsc-alias": "^1.8.10"
  },
  "dependencies": {
    "currency-symbol-map": "^5.1.0"
  }
}
