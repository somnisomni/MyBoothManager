    "@types/node": "^20.14.15",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.9.0",
    "npm-run-all2": "^6.2.2",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
