{
  "name": "@myboothmanager/backend",
  "version": "0.5.0",
  "author": "somni <me@somni.one>",
  "private": true,
  "packageManager": "pnpm@9.3.0",
  "scripts": {
    "__run": "cross-env NODE_OPTIONS=\"--import ./scripts/node/register-esm-loader.cjs\" NODE_ENV=development",
    "__run:prod": "cross-env NODE_OPTIONS=\"--import ./dist/scripts/node/register-esm-loader.cjs\" NODE_ENV=production",
    "build": "nest build",
    "start": "pnpm __run nest start",
    "start:prod": "pnpm __run:prod node dist/src/main",
    "start:dev": "pnpm start --watch --preserveWatchOutput",
    "start:debug": "pnpm start --debug --watch",
    "migrate": "cross-env NODE_ENV=development ts-node -P tsconfig.scripts.json -r tsconfig-paths/register scripts/db-migrator/migrate",
    "migrate:prod": "pnpm __run:prod node dist/scripts/db-migrator/migrate",
    "seed": "cross-env NODE_ENV=development ts-node -P tsconfig.scripts.json -r tsconfig-paths/register scripts/db-migrator/seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "pnpm lint --fix",
    "test": "jest",
    "test:watch": "pnpm test --watch",
    "test:cov": "pnpm test --coverage"
  },
  "files": [ "./dist" ],
  "dependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@fastify/cookie": "^9.3.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^7.0.4",
    "@myboothmanager/common": "workspace:^",
    "@nestjs/common": "^10.3.9",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-fastify": "^10.3.9",
    "app-root-path": "^3.1.0",
    "argon2": "^0.40.3",
    "chalk": "^4",
    "class-transformer": "^0.5.1",
    "clone-deep": "^4.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "fastify": "^4.27.0",
    "luxon": "^3.4.4",
    "mysql2": "^3.10.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "sharp": "^0.33.4",
    "umzug": "^3.8.1"
  },
  "devDependencies": {
    "@myboothmanager/dev-shared": "workspace:^",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@tsconfig/node20": "^20.1.4",
    "@types/clone-deep": "^4.0.4",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.2",
    "@types/sequelize": "^4.28.20",
    "@types/validator": "^13.11.10",
    "eslint": "^9.4.0",
    "expect-type": "^0.19.0",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
